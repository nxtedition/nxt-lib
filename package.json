{
  "name": "@nxtedition/lib",
  "version": "17.2.11",
  "license": "MIT",
  "author": "Robert Nagy <robert.nagy@boffins.se>",
  "type": "module",
  "files": [
    "hash.js",
    "ass.js",
    "rxjs/*",
    "util/*",
    "http-client.js",
    "subtract-ranges.js",
    "serializers.js",
    "platform.js",
    "elasticsearch.js",
    "merge-ranges.js",
    "http.js",
    "deepstream.js",
    "logger.js",
    "mime.js",
    "proxy.js",
    "timers.js",
    "trace.js",
    "weakCache.js",
    "couch.js",
    "app.js",
    "errors.js",
    "worker.js",
    "stream.js",
    "timeline.js",
    "docker-secrets.js"
  ],
  "scripts": {
    "prepare": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "test": "node --test __tests__ && npx tap test"
  },
  "lint-staged": {
    "*.{js,jsx,md,ts}": [
      "eslint",
      "prettier --write"
    ]
  },
  "prettier": {
    "printWidth": 100,
    "semi": false,
    "singleQuote": true
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaFeatures": {
        "ecmaVersion": 15
      }
    },
    "extends": [
      "standard",
      "prettier",
      "prettier/prettier"
    ],
    "rules": {
      "quotes": [
        "error",
        "single",
        {
          "avoidEscape": true,
          "allowTemplateLiterals": true
        }
      ]
    }
  },
  "eslintIgnore": [
    "/__tests__"
  ],
  "dependencies": {
    "@elastic/elasticsearch": "^8.11.0",
    "@elastic/transport": "^8.4.0",
    "@nxtedition/nxt-undici": "^2.0.35",
    "date-fns": "^3.1.0",
    "fast-querystring": "^1.1.1",
    "hasha": "^6.0.0",
    "http-errors": "^2.0.0",
    "json5": "^2.2.3",
    "koa-compose": "^4.1.0",
    "lodash": "^4.17.21",
    "mime": "^4.0.1",
    "moment-timezone": "^0.5.44",
    "nconf": "^0.12.1",
    "nested-error-stacks": "^2.1.1",
    "object-hash": "^3.0.0",
    "pino": "^8.17.2",
    "pino-std-serializers": "^6.2.2",
    "qs": "^6.11.1",
    "request-target": "^1.0.2",
    "smpte-timecode": "^1.3.5",
    "split-string": "^6.0.0",
    "split2": "^4.2.0",
    "toobusy-js": "^0.5.1",
    "undici": "^6.3.0",
    "url-join": "^5.0.0"
  },
  "devDependencies": {
    "@nxtedition/deepstream.io-client-js": ">=24.1.0",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.10.7",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "pinst": "^3.0.0",
    "prettier": "^3.1.1",
    "rxjs": "^7.5.6",
    "send": "^0.18.0",
    "tap": "^18.5.8"
  },
  "peerDependencies": {
    "@elastic/elasticsearch": "^8.6.0",
    "@elastic/transport": "^8.3.1",
    "@nxtedition/deepstream.io-client-js": ">=14.1.0",
    "pino": ">=7.0.0",
    "rxjs": ">=6.0.0"
  }
}
