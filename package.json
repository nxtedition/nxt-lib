{
  "name": "@nxtedition/lib",
  "version": "13.0.2",
  "license": "MIT",
  "author": "Robert Nagy <robert.nagy@boffins.se>",
  "files": [
    "hash.js",
    "ass.js",
    "rxjs/*",
    "util/*",
    "subtract-ranges.js",
    "serializers.js",
    "elasticsearch.js",
    "merge-ranges.js",
    "http.js",
    "deepstream.js",
    "logger.js",
    "mime.js",
    "trace.js",
    "weakCache.js",
    "couch.js",
    "app.js",
    "errors.js",
    "worker.js",
    "proxy.js",
    "timeline.js",
    "docker-secrets.js"
  ],
  "scripts": {
    "prepare": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "test": "node --test __tests__ && npx tap test"
  },
  "lint-staged": {
    "*.{js,jsx,md,ts}": [
      "eslint",
      "prettier --write"
    ]
  },
  "prettier": {
    "printWidth": 100,
    "semi": false,
    "singleQuote": true
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaFeatures": {
        "ecmaVersion": 2020
      }
    },
    "extends": [
      "standard",
      "prettier",
      "prettier/prettier"
    ],
    "rules": {
      "quotes": [
        "error",
        "single",
        {
          "avoidEscape": true,
          "allowTemplateLiterals": true
        }
      ]
    }
  },
  "eslintIgnore": [
    "/__tests__"
  ],
  "dependencies": {
    "date-fns": "^2.29.3",
    "fast-querystring": "^1.1.1",
    "hasha": "^5.2.2",
    "http-errors": "^2.0.0",
    "json5": "^2.2.3",
    "koa-compose": "^4.1.0",
    "lodash": "^4.17.21",
    "mime": "^3.0.0",
    "moment-timezone": "^0.5.43",
    "nconf": "^0.12.0",
    "nested-error-stacks": "^2.1.1",
    "object-hash": "^3.0.0",
    "qs": "^6.11.1",
    "request-target": "^1.0.2",
    "split-string": "^6.0.0",
    "toobusy-js": "^0.5.1",
    "undici": "^5.22.0",
    "url-join": "^4.0.0"
  },
  "devDependencies": {
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.1",
    "pinst": "^3.0.0",
    "prettier": "^2.8.7",
    "rxjs": "^7.5.6",
    "tap": "^16.3.4"
  },
  "peerDependencies": {
    "@elastic/elasticsearch": "^8.6.0",
    "@elastic/transport": "^8.3.1",
    "@nxtedition/deepstream.io-client-js": ">=14.1.0",
    "pino": ">=7.0.0",
    "rxjs": ">=6.0.0"
  },
  "optionalDependencies": {
    "linux-perf": "^0.3.1",
    "nice-napi": ">=1.0.0"
  }
}
